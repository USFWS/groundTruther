backOfEnvelopePopProjection <- function(nYears=25, nStock=3000, survival=0.8){
ind <- rep(1, nStock)
age <- rep(0, nStock)
for(i in 2:nYears){
survive <- rbinom(ind, size=1, prob=survival) * ind
ind <- c(survive, rep(1, nStock))
age <- age + 1
age <- c(age, rep(0, nStock))
}
# return(sum(ind))
return(data.frame(age, alive=ind))
}
lasPop <- backOfEnvelopePopProjection()
sum(lasPop$alive)
sum(lasPop[lasPop$age>9,]$alive)
